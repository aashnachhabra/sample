@GetMapping("/getServiceTierLookUp")
    @ResponseBody
    @ApiOperation(value = "Get the lookup records")
    public ModelAndView getServiceLookUp(@RequestParam("serviceRuleID") Integer serviceRuleID, ModelAndView modelAndView) {

        List<ServiceRuleTierResponse> serviceRuleTierResponse = cmcPdvcVarCompRecordService.lookUpServiceRuleTierMatch(serviceRuleID);

        modelAndView.setViewName("CostShareDetails");

        modelAndView.addObject("serviceRuleTier", serviceRuleTierResponse);

        return modelAndView;
    }


