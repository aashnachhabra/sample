private _setFrappeTable(forms: FormDashboardItem[]) {
    const that = this;
    const selectRowFn = this.selectRow;
    const datatable = new DataTable(this.formTable?.nativeElement, {
      columns: this.columns,
      data: forms,
      checkboxColumn: true,
      inlineFilters: true,
      layout: 'fluid',
      serialNoColumn: false,
      treeView: true,
      dynamicRowHeight: true,
      disableReorderColumn: true,
      events: {
        onCheckRow() {
          const rows: any[] = datatable.getRows();
          const checkedRows: any[] = datatable.rowmanager.getCheckedRows();
          rows.forEach(row => row[0].value = 'false');
          checkedRows.forEach(checkedRow => rows[checkedRow][0].value = 'true');
          const selectedRows = checkedRows.map(checkedRow => rows[checkedRow]);
          const selectedFormIDs = selectedRows.map(row => row[1].content);
          that.selectRow(selectedFormIDs);
        },
      },
      search: true,
      searchColumns: ['formID',
        'status',
        'documentName',
        'groupName',
        'groupNumber',
        'planNumber',
        'benefitPlan',
        'versionType',
        'careConsultant',
        'sae',
        'recentVersion',
        'createdBy',
        'createdOn',
        'modifiedBy',
        'modifiedOn'],
      noDataMessage: 'Awe Snap! No results returned. Please update your search criteria then try again.'
    });
    datatable.style.setStyle(`.dt-cell__content--header-0 > input`, { display: 'none' });
  }
