 openSelectFormModal() {
    const modalRef = this.modalService.open(SelectFormModalComponent, {
      centered: true
    });
    modalRef.componentInstance.formList = this.formList
    modalRef.result.then(result => {
      if (result) {
        this.formGenerationService.getFormByID(result.form1).subscribe((result: any) => {
          if (result) {
            this.selectedForm1 = result
            console.log("FORM 1 : ", this.selectedForm1)
          }
        })
        this.formGenerationService.getFormByID(result.form2).subscribe((result: any) => {
          if (result) {
            this.selectedForm2 = result
            console.log("FORM 2 : ", this.selectedForm2)
          }
        })
        console.log("reqst body",{ form1: this.selectedForm1, form2: this.selectedForm2 })
        this.formGenerationService.compareForms({ form1: this.selectedForm1, form2: this.selectedForm2 }).subscribe((result:any)=>{
          if(result){
            console.log("RESULT : ", result)
          }
        })
      }
    }).catch(error => {
      console.log("Compare modal dimissed")
    });
  }
