FROM docker.repo1.uhc.com/maven:3.9.6 AS builder
COPY . /usr/src/app
COPY settings.xml /root/.m2/settings.xml
WORKDIR /usr/src/app
RUN mvn clean install

FROM docker.repo1.uhc.com/tomcat:10-jdk17
RUN apt-get update && apt-get upgrade -y

COPY --from=builder /usr/src/app/target/planlibrary.war /usr/local/tomcat/webapps/planlibrary.war

RUN mkdir -m 755 /usr/local/tomcat/kafka

COPY --from=builder /usr/src/app/target/classes/cert.jks /usr/local/tomcat/kafka/cert.jks

RUN chown -R 1001:0 /usr/local/tomcat/webapps/*
RUN chmod -R 777 /usr/local/tomcat/webapps/*

ENV SPRING_PROFILES_ACTIVE=dev-gcp

EXPOSE 8080

CMD ["catalina.sh","run"]

dockerfile

