function loadViewPageData(){
	var planAssignmentIdView = localStorage.getItem('planAssignmentIdView');
	let ifpRole = document.getElementById("ifpRole").value;
	localStorage.removeItem('planAssignmentIdView');
	if(planAssignmentIdView != null && planAssignmentIdView !=""){
		showAndHide();
		$.ajax({
			type : "POST",
			contentType : "application/json",
			url : "portfolioviewsearch",
			data : planAssignmentIdView,
			dataType : 'json',
			success : function(response) {
				if (response.length !== 0) {
					$('#processLogDataTable').css('display', 'block');
					$('#noResult').css('display', 'none');
					var table = $('#processLog').DataTable();
					populateTableData(response);
					table.columns.adjust().draw();
					createViewExportRequest(planAssignmentIdView);
					if (ifpRole != 10) {
						enableButtons();
					}
				} else {
					$('#noResult').css('display', 'block');
					$('#processLogDataTable').css('display', 'none');
					$('#clearButton').prop('disabled', false);
				}
				showAndHide();
			},
			error : function(xhr) {
				$('#processLogDataTable').css('display', 'none');
				$('#noResult').css('display', 'block');
				showAndHide();
			}
		});
	}
};
this function is fetching it from local storage. what changes do i need to make here to make sure the above changes work without disrupting the functionality at all
