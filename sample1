function buildDatatable(inputdata){
    let checkFlag = false;
    const datatable = new DataTable('#datatable', {
        columns: [
            {
                name: 'Plan AssignmentId',
                id: 'planManagementId',
                editable: false,
                dropdown: false,
            },
            {
                name: 'Plan Code',
                id: 'planCode',
                editable: false,
            },
            {
                name: 'Product ID',
                id: 'productID',
                editable: false,
            },
            {
                name: 'HIOS ID',
                id: 'hiosID',
                editable: false,
            },
            {
                name: 'COC Series',
                id: 'cocSeries',
                editable: false,
            },
            {
                name: 'State',
                id: 'stateAbbr',
                editable: false,
            },
            {
                name: 'Licence',
                id: 'licenceCode',
                editable: false,
            },
            {
                name: 'Product Family',
                id: 'product',
                editable: false,
            },
            {
                name: 'Last Generated Date',
                id: 'lastGeneratedDate',
                editable: false,
            },
            {
                name: 'Release Date',
                id: 'lastGenDate',
                editable: false,
            },
            {
                name: 'Version',
                id: 'version',
                editable: false,
            },
            {
                name: 'Region',
                id: 'region',
                editable: false,
            },
        ],
        events: {
                    onCheckRow: () => {
                        let arrayrow = datatable.getRows();
                        for (var i = 0; i < arrayrow.length; i++) {
                            arrayrow[i][0].value = "false";
                        }
                        this.checkedRows = this.datatable.rowmanager.getCheckedRows();

                        console.log("checked rows::::"+this.checkedRows);

                        if (this.checkedRows.length <= 0) {
                            disabledButtons();
                        }else{
                            for(var i = 0; i< this.checkedRows.length; i++){
                                arrayrow[this.checkedRows[i]][0].value = "true";
                            }
                            enableButtons();
                        }
                    }
                  },
        data: inputdata,
        checkboxColumn: true,
        id: "planManagementId",
        inlineFilters: true,
        layout: 'fluid',
        serialNoColumn: false,
        noDataMessage: 'Awe Snap! No results returned. Please update your search criteria then try again.'
    });
    datatable.style.setStyle(`.dt-cell__content .dt-cell__content--header-0`, {padding: '0'});
    datatable.style.setStyle(`.dt-cell--header .dt-cell__content`, {overflow: 'visible'});
    datatable.style.setStyle(`.dt-cell__content--col-0`, {overflow: 'visible'});
    datatable.style.setStyle(`.dt-toast`, {top: '40px'});
    datatable.style.setStyle(`.dt-scrollable`, {'border-bottom': '1px solid #d1d8dd !important'});
    window.datatable=datatable;
}
