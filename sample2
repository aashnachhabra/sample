@PostMapping("/getServiceRuleMgmtDetails")
	@ApiOperation(value=" Get existing Service Rule Management Details")
	public ModelAndView getServiceRuleMgmtDetails(@Valid @RequestBody ServiceRuleMgmtRequest serviceRuleMgmtRequest) throws Exception {
		ModelAndView modelAndView = new ModelAndView();

		if (serviceRuleMgmtRequest == null) {
			throw new Exception("serviceRuleMgmtRequest should not be Empty or null..!");
		}
		ServiceRuleManagementDTO serviceRuleManagementDTO = serviceRuleMgmtService.getServiceRuleDetails(serviceRuleMgmtRequest);
		logger.info("ServiceRuleManagementDTO : "+serviceRuleManagementDTO.toString());
		modelAndView.addObject("serviceRuleManagementDTO",serviceRuleManagementDTO);
		modelAndView.setViewName("ServiceRuleEditManagement");
		return modelAndView;
	}

