<select id="Plan"
                            class="form-control filterTable multiSelectField"
                            multiple="multiple">
                            <c:forEach var="pln" items="${planMaster}" varStatus="status">
                                <c:if test="${ruleType == 18 || status.index < 2}">
                                    <option value="${pln.planTypeName}"
                                        <c:if test="${fn:contains(planIn, pln.planTypeName)}">selected</c:if>>
                                        ${pln.planTypeName}
                                    </option>
                                </c:if>
                            </c:forEach>
                        </select>

