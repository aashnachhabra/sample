function validateClone() {
    var rows = document.getElementById('newmaxtbl').rows;
    var j = 0;
    for (var i = 1; i < rows.length; i++) {
        var benefitCategoryName = document.getElementById("benefitCategoryName_" + i).value;
        var appliesTo = document.getElementById("appliesTo_" + i).checked;
        var crossApplies = document.getElementById("crossApplies_" + i).checked;
        var maxType = document.getElementById("maxType_" + i).value;
        var maxValue = document.getElementById("maxValue_" + i).value;
        var freqType = document.getElementById("freqType_" + i).value;
        var freqValue = document.getElementById("freqValue_" + i).value;
        var coc = document.getElementById("rescoc_" + i).value;
        var state = document.getElementById("resstateName_" + i).value;
        var stand = document.getElementById("resstandard_" + i).value;

        document.getElementById("cloneCoc").value = coc;
        document.getElementById("cloneState").value = state;
        document.getElementById("cloneStandard").value = stand;
        document.getElementById("cloneBenefitCategoryName").value = benefitCategoryName;
        document.getElementById("cloneAppliesTo").value = appliesTo;
        document.getElementById("cloneCrossAppliesTo").value = crossApplies;
        document.getElementById("cloneMaxType").value = maxType;
        document.getElementById("cloneMaxValue").value = maxValue;
        document.getElementById("cloneFreqType").value = freqType;
        document.getElementById("cloneFreqValue").value = freqValue;
        j++;
    }
    $("#maxCloneModal").modal("toggle");
}

for some reason my clone modal isnt opening this is the function

